{
	"title": "SELECT dept, avg WHERE room seats > 150 ,sort by rooms full name",
	"input": {
		"WHERE": {
			"AND": [{
				"IS": {
					"rooms_furniture": "*Tables*"
				}
			}, {
				"GT": {
					"rooms_seats": 300
				}
			}]
		},
		"OPTIONS": {
			"COLUMNS": [
				"rooms_shortname",
				"maxSeats"
			],
			"ORDER": {
				"dir": "DOWN",
				"keys": ["maxSeats"]
			}
		},
		"TRANSFORMATIONS": {
			"GROUP": ["rooms_shortname"],
			"APPLY": [{
				"maxSeats": {
					"MAX": "rooms_seats"
				}
			}]
		}
	},
	"errorExpected": false,
	"with": [{"rooms_shortname":"OSBO","maxSeats":442},{"rooms_shortname":"HEBB","maxSeats":375},{"rooms_shortname":"LSC","maxSeats":350}]}
